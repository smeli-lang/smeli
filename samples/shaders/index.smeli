@plugin("@smeli/plugin-ui")

# Shader demo

ui: ui {
  theme: default_themes.dark
  page: ui.surface {
    direction: "row"
    responsive: true
    item0: ui.shader {
    }
    item1: ui.surface {
      direction: "column"
      elevation: 2
      item0: ui.outline {}
    }
  }
}
